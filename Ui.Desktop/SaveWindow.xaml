<Window x:Class="De.HsFlensburg.ClientApp049.Ui.Desktop.SaveWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:De.HsFlensburg.ClientApp049.Ui.Desktop"
        mc:Ignorable="d"
        Title="SaveWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="95*"/>
            <ColumnDefinition Width="169*"/>
        </Grid.ColumnDefinitions>

        <Button Content="Export" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="80,110,0,109" Width="200" Height="200"/>
        <Button Content="Import" HorizontalAlignment="Right" VerticalAlignment="Center" Margin="0,110,80,109" Width="200" Height="200" Grid.Column="1"/>
        <Label Content="Export/Import" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="90"/>
    </Grid>
</Window>
